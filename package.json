    "markdown-to-jsx": "^7.4.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.8",
    "use-dark-mode": "^2.3.1"
