    "jest-environment-jsdom": "^29.7.0",
    "react-snap": "^1.23.0",
    "rimraf": "^5.0.5",
    "sass": "^1.75.0",
    "source-map-explorer": "^2.5.3"
  },
  "overrides": {
    "use-dark-mode": {
